(constraint (= (f "01 Sep 2012") "Saturday #1 September 2012"))
(constraint (= (f "5 June 2016") "Sunday #1 June 2016"))
(constraint (= (f "02-Jan-1998") "Friday #1 January 1998"))
(constraint (= (f "12171973") "Monday #3 December 1973"))
(constraint (= (f "1932 97") "Wednesday #1 April 1932"))
(constraint (= (f "Mar 1983 Tue 3") "Tuesday #3 March 1983"))
(constraint (= (f "Thu wk 10 in 2039") "Thursday #2 March 2039"))
