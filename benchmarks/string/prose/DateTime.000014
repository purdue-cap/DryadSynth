
    (set-logic SLIA)
    (synth-fun f ((arg0 String)) String 
        (
            (Start String (ntString)) 
            (ntString String ("day #" "nday #" "day #3" "December" "Friday #" "day #2" "uary 201" "1 January" "sday #" "day #1" "esday #" "day #1 J" arg0
                (str.++ ntString ntString) 
                (str.replace ntString ntString ntString) 
                (str.at ntString ntInt)
                (int.to.str ntInt)
                (ite ntBool ntString ntString)
                (str.substr ntString ntInt ntInt)
            )) 
            (ntInt Int (
                1 0 -1
                (+ ntInt ntInt)
                (- ntInt ntInt)
                (str.len ntString)
                (str.to.int ntString)
                (ite ntBool ntInt ntInt)
                (str.indexof ntString ntString ntInt)
            )) 
            (ntBool Bool (
                true false
                (= ntInt ntInt)
                (str.prefixof ntString ntString)
                (str.suffixof ntString ntString)
                (str.contains ntString ntString)
            ))
        )
    )
(constraint (= (f "17 Dec 1973") "Monday #3 December 1973"))
(constraint (= (f "15 Aug 1947") "Friday #3 August 1947"))
(constraint (= (f "Jan 10 1975") "Friday #2 January 1975"))
(constraint (= (f "1 January 2014") "Wednesday #1 January 2014"))
